                new Chart(ctx4, { type: 'doughnut', data: doughnutData, options: doughnutOptions });
              }
            });
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {

            $.ajax({
                url: '../BaseAdmin/GetDadosFornecedorUFLista'
                , type: 'POST'
                , success: function (result) {
                var barData = {
                    labels: result.ufs,
                    datasets: [
                        {
                            label: "Fornecedores por UF",
                            backgroundColor: 'rgba(26,179,148,0.5)',
                            pointBorderColor: "#fff",
                            data: result.valores
                        }
                    ]
                };

                var barOptions = {
                    responsive: true
                };

                var ctx2 = document.getElementById("barChartFornecedorUF").getContext("2d");
                new Chart(ctx2, { type: 'bar', data: barData, options: barOptions });
              }
            });
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {

            $.ajax({
                url: '../BaseAdmin/GetDadosFornecedorCidadeLista'
                , type: 'POST'
                , success: function (result) {
                var barData = {
                    labels: result.cids,
                    datasets: [
                        {
                            label: "Fornecedores por Cidade",
                            backgroundColor: 'rgba(26,179,148,0.5)',
                            pointBorderColor: "#fff",
                            data: result.valores
                        }
                    ]
                };

                var barOptions = {
                    responsive: true
                };

