@using Presentation.Extensions
@using CrossCutting
@model ERP_Condominios_Solution.ViewModels.UsuarioViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div id="content" class="wraper container-fluid">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="col-lg-12 col-md-12 col-sm-12">
            @*Atalhos*@
            <div class="row">
                <h3 style="color:olivedrab; font-weight:bold">Dashboard - Cadastros</h3>
                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                    <br />
                    <div class="col-lg-12 col-md-12 col-xs-12">
                        @Html.ButtonLinkIndex(text: "Máquinas de Cartão", action: "MontarTelaMaquina", controller: "Maquina", buttonClass: "primary")
                        @Html.ButtonLinkIndex(text: "Plataformas de Entrega", action: "MontarTelaPlataformaEntrega", controller: "PlataformaEntrega", buttonClass: "primary")
                        @Html.ButtonLinkIndex(text: "Clientes", action: "MontarTelaCliente", controller: "Cliente", buttonClass: "success")
                        @Html.ButtonLinkIndex(text: "Fornecedores", action: "MontarTelaFornecedor", controller: "Fornecedor", buttonClass: "success")
                        @Html.ButtonLinkIndex(text: "Produtos", action: "MontarTelaProduto", controller: "Produto", buttonClass: "success")
                        @Html.ButtonLinkIndex(text: "Fichas Técnicas", action: "MontarTelaFT", controller: "FichaTecnica", buttonClass: "success")
                        @Html.ButtonLinkIndex(text: "Voltar", controller: "BaseAdmin", action: "VoltarDashboard")
                    </div>
                    <br /><br />
                </div>
            </div>

            @*Estatísticas*@
            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-12">
                    <div class="ibox-content text-center">
                        <h1>@((String)Session["Nome"])</h1>
                        <div class="m-b-sm">
                            <img alt="image" class="img-circle" src="@Url.Content(Model.USUA_AQ_FOTO)" style="width: auto; height: auto; max-height: 90px; max-width: 90px;">
                        </div>
                        <p class="font-bold">@((String)Session["NomeGreeting"])</p>

                        <div class="text-center">
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <span class="label label-warning pull-right">Hoje</span>
                            <h5>Resumo Cadastros</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="row">
                                <div class="col-xs-4">
                                    <small class="stats-label">Clientes</small>
                                    <h4>@ViewBag.Clientes</h4>
                                </div>
                                <div class="col-xs-4">
                                    <small class="stats-label">Fornecedores</small>
                                    <h4>@ViewBag.Fornecedores</h4>
                                </div>
                                <div class="col-xs-4">
                                    <small class="stats-label">Produtos</small>
                                    <h4>@ViewBag.Produtos</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

