@using Presentation.Extensions
@model ERP_Condominios_Solution.ViewModels.EmpresaViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="modalContainer">
        <div id="modal" class="modal" tabindex="-1" role="dialog">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        @*titulo gerado dinamicamente*@
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        @*Gerado dinamicamente*@
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="page-title">
        <h3 class="title">Configuração da Empresa</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li class="active">
                <strong>Configuração da Empresa</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
            <br />
            <div class="col-lg-12 col-md-12 col-xs-12">
                @Html.ButtonLinkIndex(text: "Máquinas de Cartão", action: "MontarTelaMaquina", controller: "Maquina", buttonClass: "primary")
                @Html.ButtonLinkIndex(text: "Plataformas de Entrega", action: "MontarTelaPlataformaEntrega", controller: "PlataformaEntrega", buttonClass: "primary")
                @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarDash", controller: "Empresa")
            </div>
            <br /><br />
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Configuração da Empresa</h5>
                </div>

                <div class="ibox-content">
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Máquinas de Cartões Associadas</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Anexos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-5">Indicadores</a></li>
                        </ul>
                        <div class="tab-content">

                            @*Dados Gerais*@
                            <div id="tab-1" class="tab-pane active">
                                <div class="panel-body">
                                    @using (Html.BeginForm("MontarTelaEmpresa", "Empresa", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row" style="width: 100%;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                    @if (ViewBag.Perfil == "ADM" || ViewBag.Perfil == "GER")
                                                    {
                                                        @Html.ButtonActionSubmit(text: "Confirma Alteração da Configuração da Empresa", htmlAttributes: new { id = "basictoast" })
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row" style="width: 100%;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <div class="row">
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                            @Html.HiddenFor(m => m.EMPR_IN_ATIVO)
                                                            @Html.HiddenFor(m => m.EMPR_DT_CADASTRO)
                                                            @Html.HiddenFor(m => m.EMPR_CD_ID)
                                                            Nome*
                                                            @Html.TextBoxFor(m => m.EMPR_NM_NOME, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NM_NOME, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Razão Social
                                                            @Html.TextBoxFor(m => m.EMPR_NM_RAZAO, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NM_RAZAO, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            CNPJ*
                                                            @Html.TextBoxFor(m => m.EMPR_NR_CNPJ, new { @class = "form-control", @id = "cnpj", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NR_CNPJ, "", new { @style = "color:red" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Regime Tributário*
                                                            @Html.DropDownListFor(m => m.RETR_CD_ID, (SelectList)ViewBag.Regimes, "Selecionar", new { @class = "form-control", autofocus = true, onchange="GetRegime($(this).val())" })
                                                            @Html.ValidationMessageFor(m => m.RETR_CD_ID, "", new { @style = "color:red" })
                                                        </div>
                                                        @*<div class="col-lg-2 col-md-2 col-xs-12">
                                                            Alíquota Real (%)
                                                            @Html.TextBoxFor(m => m.REGIME_TRIBUTARIO.RETR_VL_ALIQUOTA, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                                                            @Html.ValidationMessageFor(m => m.EMPR_VL_IMPOSTO_MEI, "", new { @style = "color:red" })
                                                        </div>*@
                                                        @if (Model.EMPR_VL_IMPOSTO_MEI == Model.REGIME_TRIBUTARIO.RETR_VL_ALIQUOTA)
                                                        {
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Alíquota Imposto (%)
                                                                @Html.TextBoxFor(m => m.EMPR_VL_IMPOSTO_MEI, new { @class = "form-control", autofocus = true, @id= "aliquota" })
                                                                @Html.ValidationMessageFor(m => m.EMPR_VL_IMPOSTO_MEI, "", new { @style = "color:red" })
                                                            </div>
                                                        }
                                                        else
                                                        {
                                                            <div class="col-lg-2 col-md-2 col-xs-12" style="background-color:beige; color: green; font-weight: bold">
                                                                Alíquota Imposto (%)
                                                                @Html.TextBoxFor(m => m.EMPR_VL_IMPOSTO_MEI, new { @class = "form-control", autofocus = true, @id= "aliquota" })
                                                                @Html.ValidationMessageFor(m => m.EMPR_VL_IMPOSTO_MEI, "", new { @style = "color:red" })
                                                            </div>
                                                        }
                                                    </div>
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Insc.Estadual
                                                            @Html.TextBoxFor(m => m.EMPR_NR_INSCRICAO_ESTADUAL, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NR_INSCRICAO_ESTADUAL, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Insc.Municipal
                                                            @Html.TextBoxFor(m => m.EMPR_NR_INSCRICAO_MUNICIPAL, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NR_INSCRICAO_MUNICIPAL, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Opera Cartão?*
                                                            @Html.DropDownListFor(m => m.EMPR_IN_OPERA_CARTAO, (SelectList)ViewBag.Opera, "Selecionar", new { @class = "form-control", autofocus = true, @id = "opera", onchange = "ValidaDiv($(this).val())" })
                                                            @Html.ValidationMessageFor(m => m.EMPR_IN_OPERA_CARTAO, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Paga Comissão?*
                                                            @Html.DropDownListFor(m => m.EMPR_IN_PAGA_COMISSAO, (SelectList)ViewBag.Comissao, "Selecionar", new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.EMPR_IN_PAGA_COMISSAO, "", new { @style = "color:red" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group" style="margin: 0 -15px 0 -15px;" id="containerCep">
                                                        <div class="col-lg-1 col-md-1 col-xs-12">
                                                            CEP
                                                            @Html.TextBoxFor(m => m.EMPR_NR_CEP, new { @class = "form-control", autofocus = true, @id = "cep" })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NR_CEP, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            <span class="btn btn-warning btn-sm" style="margin-bottom: -45px;" id="btnBuscaCep" onclick="PesquisaCep($('#cep').val(), $(this), 1)">Buscar CEP</span>
                                                        </div>
                                                    </div>
                                                    <div class="form-group" style="margin: 30px -15px 0 -15px;" id="containerEnd">
                                                        <div class="col-lg-4 col-md-4 col-xs-12">
                                                            Endereço
                                                            @Html.TextBoxFor(m => m.EMPR_NM_ENDERECO, new { @class = "form-control", autofocus = true, @id = "endereco" })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NM_ENDERECO, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Bairro
                                                            @Html.TextBoxFor(m => m.EMPR_NM_BAIRRO, new { @class = "form-control", autofocus = true, @id = "bairro" })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NM_BAIRRO, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Cidade
                                                            @Html.TextBoxFor(m => m.EMPR_NM_CIDADE, new { @class = "form-control", autofocus = true, @id = "cidade" })
                                                            @Html.ValidationMessageFor(m => m.EMPR_NM_CIDADE, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            UF
                                                            @Html.DropDownListFor(m => m.UF_CD_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", autofocus = true, @id = "uf" })
                                                            @Html.ValidationMessageFor(m => m.UF_CD_ID, "", new { @style = "color:red" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row" style="width: 100%;">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="PJ">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Patrimônio Líquido (R$)*
                                                    @Html.TextBoxFor(m => m.PatrimonioText, "{0:n2}", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PatrimonioText, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Perc.Venda Débito (%)
                                                    @Html.TextBoxFor(m => m.EMPR_PC_VENDA_DEBITO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.EMPR_PC_VENDA_DEBITO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Perc.Venda Crédito (%)
                                                    @Html.TextBoxFor(m => m.EMPR_PC_VENDA_CREDITO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.EMPR_PC_VENDA_CREDITO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Perc.Venda Dinheiro (%)
                                                    @Html.TextBoxFor(m => m.EMPR_PC_VENDA_DINHEIRO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.EMPR_PC_VENDA_DINHEIRO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>

                            <div id="tab-3" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row" id="maquina">
                                        <hr />
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" >
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Máquinas de Cartões Associadas</h4>
                                                <br />
                                                @if (Model.EMPRESA_MAQUINA != null && Model.EMPRESA_MAQUINA.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Nome</th>
                                                                <th>Marca</th>
                                                                <th>Perc.Crédito (%)</th>
                                                                <th>Perc.Débito (%)</th>
                                                                <th>Perc.Antecipação (%)</th>
                                                                @if (ViewBag.Perfil == "ADM" || ViewBag.Perfil == "GER")
                                                                {
                                                                    <th class="td-one-action">Ações</th>
                                                                }
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.EMPRESA_MAQUINA))
                                                            {
                                                                <tr>
                                                                    <td>@cond.MAQUINA.MAQN_NM_NOME</td>
                                                                    <td>@cond.MAQUINA.MAQN_NM_PROVEDOR</td>
                                                                    <td>@CrossCutting.Formatters.DecimalFormatter(cond.MAQUINA.MAQN_PC_CREDITO)</td>
                                                                    <td>@CrossCutting.Formatters.DecimalFormatter(cond.MAQUINA.MAQN_PC_DEBITO)</td>
                                                                    <td>@CrossCutting.Formatters.DecimalFormatter(cond.MAQUINA.MAQN_PC_ANTECIPACAO.Value)</td>
                                                                    @if (ViewBag.Perfil == "ADM" || ViewBag.Perfil == "GER")
                                                                    {
                                                                        <td class="td-one-action">
                                                                            @if (cond.EMMA_IN_ATIVO == 1)
                                                                            {
                                                                                <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/Empresa/ExcluirEmpresaMaquina/@cond.EMMA_CD_ID')" title="Excluir"></span>
                                                                            }
                                                                            else
                                                                            {   
                                                                                <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-refresh" onclick="ShowModalReativar('/Empresa/ReativarEmpresaMaquina/@cond.EMMA_CD_ID')" title="Reativar"></span>
                                                                            }
                                                                        </td>
                                                                    }
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        @if (ViewBag.Perfil == "ADM" || ViewBag.Perfil == "GER")
                                        {
                                            @Html.ButtonLinkIndex(text: "Associar nova Máquina de Cartão", action: "IncluirEmpresaMaquina", controller: "Empresa")
                                        }
                                    </div>
                                </div>
                            </div>
                            
                            @*Imagens - Foto e Anexos*@
                            <div id="tab-2" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <hr />
                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                            <h4>Anexos</h4>
                                            <br />
                                            <div class="form-group">
                                                @using (Html.BeginForm("UploadFileEmpresa", "Empresa", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { enctype = "multipart/form-data", @id = "pwd-container2" }))
                                                {
                                                    <div class="ibox-content">
                                                        @if (Model.EMPRESA_ANEXO.Count > 0)
                                                        {
                                                            <table class="table table-striped table-bordered table-hover dataTables-example">
                                                                <thead style="background-color:lightsteelblue">
                                                                    <tr>
                                                                        <th>Título</th>
                                                                        <th>Data</th>
                                                                        <th>Tipo</th>
                                                                        <th class="td-one-action">Ações</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    @foreach (var cond in (Model.EMPRESA_ANEXO))
                                                                    {
                                                                        <tr>
                                                                            <td>@cond.EMAN_NM_TITULO</td>
                                                                            <td>@cond.EMAN_DT_ANEXO.ToShortDateString()</td>
                                                                            @if (cond.EMAN_IN_TIPO == 1)
                                                                            {
                                                                                <td>Imagem</td>
                                                                            }
                                                                            else if (cond.EMAN_IN_TIPO == 2)
                                                                            {
                                                                                <td>Vídeo</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Arquivo</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @if (cond.EMAN_IN_TIPO == 1 || cond.EMAN_IN_TIPO == 2)
                                                                                {
                                                                                    @Html.TableLinkDetails(cond.EMAN_CD_ID, "VerAnexoEmpresa", "fa-camera", "Visualizar")
                                                                                }
                                                                                @if (cond.EMAN_IN_TIPO == 3)
                                                                                {
                                                                                    <a title="Visualizar" class="fa fa-camera fa-lg" href="~/PdfViewer/web/viewer.html?file=@(cond.EMAN_AQ_ARQUIVO.Replace("~", ""))&ReturnUrl=@Request.Url.AbsolutePath" target="_blank" ></a>
                                                                                }
                                                                                @Html.TableLinkDetails(cond.EMAN_CD_ID, "DownloadEmpresa", "fa-download", "Download")
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        }
                                                        <div class="form-group">
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                <input style="float: left; margin-right: 5px;" type="file" name="file" id="file" class="btn btn-sm btn-warning" />
                                                                <input style="float: left; margin-right: 5px;" type="submit" value="Incluir Anexo na Empresa" class="btn btn-sm btn-success" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Indicadores*@
                            <div id="tab-5" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <h3>A DEFINIR</h3>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Styles {
    <link href="~/Content/themes/base/all.css" rel="stylesheet" />
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/plugins/jasnyBootstrapStyles")
    @Styles.Render("~/plugins/toastrStyles")
}
@section Scripts {
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/plugins/jasnyBootstrap")
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.4.1/jquery.maskedinput.js"></script>

    <script>
        $(function () {
            limpaModal();
        });

        function limpaModal() {
            $('#btnModal').remove();
            $('#modalTitle').remove();
        }

        function ShowModalExcluir(url) {
            $('#modal').find('.modal-footer').prepend('<a id="btnModal" alt="Desativar" class="btn btn-primary" href="' + url + '" target="_self" title="Desativar">Confirmar</a>');
            $('#modal').find('.modal-body').html('Confirma exclusão de máquina de cartão?');
            $('#modal').find('.modal-header').prepend('<h5 id="modalTitle" class="modal-title">Confimar Exclusão</h5>');
            $('#modal').modal('show');
        }

        function ShowModalReativar(url) {
            $('#modal').find('.modal-footer').prepend('<a id="btnModal" alt="Reativar" class="btn btn-primary" href="' + url + '" target="_self" title="Reativar">Confirmar</a>');
            $('#modal').find('.modal-body').html('Confirma reativação de máquina de cartão?');
            $('#modal').find('.modal-header').prepend('<h5 id="modalTitle" class="modal-title">Confimar Reativação</h5>');
            $('#modal').modal('show');
        }
    </script>

    <script>
        function GetRegime(id) {
            var data = {
                id: id
            }

            $.ajax({
                url: '../Empresa/GetRegime'
                , type: 'POST'
                , data: data
                , success: function (r) {
                    $('#aliquota > input').val(r.aliquota);
                    $('#aliquota').show();
                }
            });
        }
    </script>

    <script type="text/javascript">
        $(document).ready(function () {

            $('.dataTables-example').DataTable({
                pageLength: 25,
                dom: '<"html5buttons"B>lTfgitp',
                buttons: [
                    { extend: 'excel', title: 'Maquina_Planilha' },
                    { extend: 'pdf', title: 'Maquina_PDF' },

                    {
                        extend: 'print',
                        customize: function (win) {
                            $(win.document.body).addClass('white-bg');
                            $(win.document.body).css('font-size', '10px');

                            $(win.document.body).find('table')
                                .addClass('compact')
                                .css('font-size', 'inherit');
                        }
                    }
                ]

            });

        });

    </script>
    <script type="text/javascript">
        $(function () {
            $("#cpf").mask("999.999.999-99");
            $("#cnpj").mask("99.999.999/9999-99");
            $("#cep").mask("99999-999");
            $('#tel').mask("(99) 9999-9999");
            $('#cel').mask("(99) 99999-9999");
            $('#wpp').mask("(99) 99999-9999");
        });
    </script>

    <script>
        function PesquisaCep(cep, btn, tipoEnd) {
            data = {
                cep: cep
                , tipoEnd: tipoEnd
            }

            $('#btnBuscaCep').attr("disabled", "disabled");

            $.ajax({
                url: '../Empresa/PesquisaCEP_Javascript'
                , type: 'POST'
                , data: data
                , success: function (r) {
                    $('#endereco').val(r.EMPR_NM_ENDERECO);
                    $('#bairro').val(r.EMPR_NM_BAIRRO);
                    $('#cidade').val(r.EMPR_NM_CIDADE);

                    $('#uf option[value="' + r.UF_CD_ID + '"]').attr("selected", "selected");
                    $('#btnBuscaCep').removeAttr("disabled");
                }
            });
        }
    </script>
    <script type="text/javascript">
        function ValidaDiv(valOpera) {
            if (valOpera == '1') {
                $("#maquina").show();
            } else if (valOpera == '2') { //Se o tempo for mé igual a 2 mostra a divCpf
                $("#maquina").hide();
            } 
        }

        $(document).ready(function () {
            ValidaDiv($('#opera').val());
        });
    </script>

    <script type="text/javascript">
        $(document).ready(function () {

            var i = -1;
            var toastCount = 0;
            var $toastlast;
            var getMessage = function () {
                var msg = 'Hi, welcome to Inspinia. This is example of Toastr notification box.';
                return msg;
            };

            $('#basictoast').click(function () {
                // Display a success toast, with a title
                toastr.success('Edição em andamento!')
            });
            $('#showtoast').click(function () {
                var shortCutFunction = $("#toastTypeGroup input:radio:checked").val();
                var msg = $('#message').val();
                var title = $('#title').val() || '';
                var $showDuration = $('#showDuration');
                var $hideDuration = $('#hideDuration');
                var $timeOut = $('#timeOut');
                var $extendedTimeOut = $('#extendedTimeOut');
                var $showEasing = $('#showEasing');
                var $hideEasing = $('#hideEasing');
                var $showMethod = $('#showMethod');
                var $hideMethod = $('#hideMethod');
                var toastIndex = toastCount++;
                toastr.options = {
                    closeButton: $('#closeButton').prop('checked'),
                    debug: $('#debugInfo').prop('checked'),
                    progressBar: $('#progressBar').prop('checked'),
                    positionClass: $('#positionGroup input:radio:checked').val() || 'toast-top-right',
                    onclick: null
                };
                if ($('#addBehaviorOnToastClick').prop('checked')) {
                    toastr.options.onclick = function () {
                        alert('You can perform some custom action after a toast goes away');
                    };
                }
                if ($showDuration.val().length) {
                    toastr.options.showDuration = $showDuration.val();
                }
                if ($hideDuration.val().length) {
                    toastr.options.hideDuration = $hideDuration.val();
                }
                if ($timeOut.val().length) {
                    toastr.options.timeOut = $timeOut.val();
                }
                if ($extendedTimeOut.val().length) {
                    toastr.options.extendedTimeOut = $extendedTimeOut.val();
                }
                if ($showEasing.val().length) {
                    toastr.options.showEasing = $showEasing.val();
                }
                if ($hideEasing.val().length) {
                    toastr.options.hideEasing = $hideEasing.val();
                }
                if ($showMethod.val().length) {
                    toastr.options.showMethod = $showMethod.val();
                }
                if ($hideMethod.val().length) {
                    toastr.options.hideMethod = $hideMethod.val();
                }
                if (!msg) {
                    msg = getMessage();
                }
                $("#toastrOptions").text("Command: toastr["
                    + shortCutFunction
                    + "](\""
                    + msg
                    + (title ? "\", \"" + title : '')
                    + "\")\n\ntoastr.options = "
                    + JSON.stringify(toastr.options, null, 2)
                );
                var $toast = toastr[shortCutFunction](msg, title); // Wire up an event handler to a button in the toast, if it exists
                $toastlast = $toast;
                if ($toast.find('#okBtn').length) {
                    $toast.delegate('#okBtn', 'click', function () {
                        alert('you clicked me. i was toast #' + toastIndex + '. goodbye!');
                        $toast.remove();
                    });
                }
                if ($toast.find('#surpriseBtn').length) {
                    $toast.delegate('#surpriseBtn', 'click', function () {
                        alert('Surprise! you clicked me. i was toast #' + toastIndex + '. You could perform an action here.');
                    });
                }
            });
            function getLastToast() {
                return $toastlast;
            }
            $('#clearlasttoast').click(function () {
                toastr.clear(getLastToast());
            });
            $('#cleartoasts').click(function () {
                toastr.clear();
            });

        });
    </script>

    @if (ViewBag.Message != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                  alert("@ViewBag.Message");
            };
        </script>
    }
}
